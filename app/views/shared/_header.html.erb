<header class='top-page-header'>
  <div class="title">
    <%= link_to "title", root_path, class:"title-icon"  %>
  </div>
  <div class="list">
    <%# if user_sign_in? %>
       <li><%= link_to '#', '#', class:"user-nickname" %></li>
       <li><%= link_to "ログアウト", '#', method: :'#', class: "logout" %></li>
    <%# else %>
      <li><%= link_to "ログイン", '#', class:"login" %></li>
      <li><%= link_to "新規登録", new_user_registration_path, class: "sign-up" %></li>
    <%# end %>
  </div>
</header>